//Variables
@ColorBase:#0f2847;
@blanco:white;
@aumento:#111; /* Color que incrementamos */
@baseFontSize: 16;
@azul:#135798;
@azul-rey:#0fa1e0;
@azul-bkg:#6796C2;
@negro:black;
@rojo:#be1d30;
@verde:#6f9e46;
@mobile:320px;
@tablet:481px;
@desktop:769px;
@base:800px;
@BigScreen:1140px;

//Mixins
.font-size(@sizeValue){
@pxValue: (@sizeValue * 1.6);
@remValue:(@sizeValue / 10);
font-size: ~"@{pxValue}px";
font-size: ~"@{remValue}rem";
}
.BackgroundColor (@n:1) {
  background-color: (@ColorBase + (@aumento * @n));
}
.color (@n) {
  color: (@ColorBase + (@aumento * @n));
}

.TextShadow(@x:1px, @y:1px, @blur:2px, @color:@ColorBase) {
	text-shadow: @x @y @blur @color;
}
.gradient(@ColorInicial:@ColorBase, @ColorFinal:@blanco){
	background: @ColorInicial; /* Old browsers */
    background: -moz-linear-gradient(top,  @ColorInicial 0%, @ColorFinal 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,@ColorInicial), color-stop(100%,@ColorFinal)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  @ColorInicial 0%,@ColorFinal 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  @ColorInicial 0%,@ColorFinal 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  @ColorInicial 0%,@ColorFinal 100%); /* IE10+ */
    background: linear-gradient(to bottom,  @ColorInicial 0%,@ColorFinal 100%); /* W3C */
    filter: ~"progid:DXImageTransform.Microsoft.gradient(startColorstr='@{ColorInicial}', endColorstr='@{ColorFinal}', GradientType=0 )"; /* IE6-8 */
	-ms-filter: ~"progid:DXImageTransform.Microsoft.gradient(startColorstr='@{ColorInicial}',endColorstr='@{ColorFinal}',GradientType=0 )"; /* IE6-8 */
}

.GradientX(@ColorInicial:rgba(255,255,255,0), @ColorFinal:@blanco){
	background: -moz-linear-gradient(left, @ColorInicial 0%, @ColorFinal 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, right top, color-stop(0%, @ColorInicial), color-stop(100%,rgba(255,255,255,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(left,  @ColorInicial 0%, @ColorFinal 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(left,  @ColorInicial 0%, @ColorFinal 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(left,  @ColorInicial 0%, @ColorFinal 100%); /* IE10+ */
    background: linear-gradient(to right,  @ColorInicial 0%, @ColorFinal 100%); /* W3C */
    filter: ~"progid:DXImageTransform.Microsoft.gradient( startColorstr='@{ColorInicial}', endColorstr='@{ColorFinal}',GradientType=1 )"; /* IE6-9 */
    -ms-filter: ~"progid:DXImageTransform.Microsoft.gradient( startColorstr='@{ColorInicial}', endColorstr='@{ColorFinal}',GradientType=1 )"; /* IE6-8 */

}
.perspective(@valor){
	-webkit-perspective:@valor;
	-moz-perspective: 	@valor;
	-ms-perspective: 	@valor;
	perspective: 		@valor;
}
.transition (@transition) {
	-webkit-transition: @transition;  
	-moz-transition:    @transition;
	transition:    		@transition;
}
.BorderRadius(@Radio){
	-webkit-border-radius: @Radio;
	-moz-border-radius: @Radio;
	border-radius: @Radio;
}
.border-radiuses (@topright: 0, @bottomright: 0, @bottomleft: 0, @topleft: 0) {
	-webkit-border-top-right-radius:    @topright;
	-webkit-border-bottom-right-radius: @bottomright;
	-webkit-border-bottom-left-radius:  @bottomleft;
	-webkit-border-top-left-radius:     @topleft;

	-moz-border-radius-topright:        @topright;
	-moz-border-radius-bottomright:     @bottomright;
	-moz-border-radius-bottomleft:      @bottomleft;
	-moz-border-radius-topleft:         @topleft;

	border-top-right-radius:            @topright;
	border-bottom-right-radius:         @bottomright;
	border-bottom-left-radius:          @bottomleft;
	border-top-left-radius:             @topleft;

	-moz-background-clip:    padding; 
	-webkit-background-clip: padding-box; 
	background-clip:         padding-box; 
}
.BoxShadow(@string){
	-webkit-box-shadow:@string;
	-moz-box-shadow:@string;
	box-shadow:@string;
}
.transform(@string){
	-webkit-transform: 	@string;
	-moz-transform: 	@string;
	-ms-transform: 		@string;
	-o-transform: 		@string;
}
.opacity(@opacity: 0.5) {
  -moz-opacity: @opacity;
  -khtml-opacity: @opacity;
  -webkit-opacity: @opacity;
  opacity: @opacity;
  @opperc: @opacity * 100;
  -ms-filter: ~"progid:DXImageTransform.Microsoft.Alpha(opacity=@{opperc})";
  filter: ~"alpha(opacity=@{opperc})";
}

